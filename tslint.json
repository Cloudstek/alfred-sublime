{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "no-console": false,
      "interface-name": false,
      "ordered-imports": false,
      "variable-name": false,
      "max-line-length": {
        "options": { "limit": 120 },
        "severity": "warning"
      },
      "quotemark": [true, "single", "avoid-escape", "avoid-template"]
    },
    "rulesDirectory": []
}
